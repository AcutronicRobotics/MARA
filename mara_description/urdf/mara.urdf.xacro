      <parent link="${prefix}motor3_link"/>
      <child link="${prefix}motor4_link"/>
      <axis xyz="0 0 1"/>
      <limit effort="69" lower="-3.14" upper="3.14" velocity="1.57"/>
      <dynamics damping="0" friction="0"/>
    </joint>

    <joint name="${prefix}motor5" type="revolute">
